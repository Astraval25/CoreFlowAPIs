meta {
  name: Create Customer Item
  type: http
  seq: 1
}

post {
  url: {{astraval.com}}/api/companies/{{companyId}}/customers/1/items
  body: json
  auth: bearer
}

auth:bearer {
  token: {{adminToken}}
}

body:json {
  {
    "itemId": 1,
    "salesPrice": 100.00,
    "salesDescription": "Special price for customer 1"
  }
  
}

settings {
  encodeUrl: true
  timeout: 0
}
