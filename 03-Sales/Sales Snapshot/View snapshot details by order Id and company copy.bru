meta {
  name: View snapshot details by order Id and company copy
  type: http
  seq: 1
}

get {
  url: {{astraval.com}}/api/companies/{{companyId}}/orders/snapshot?orderReference=25&status=VIEWED
  body: none
  auth: bearer
}

params:query {
  orderReference: 25
  status: VIEWED
}

auth:bearer {
  token: {{adminToken}}
}

settings {
  encodeUrl: true
  timeout: 0
}
